<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment2">

    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.theirfather.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_latestAudioFragment"
            app:destination="@id/latestAudioFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_browseAudioFragment"
            app:destination="@id/browseAudioFragment" />
    </fragment>
    <fragment
        android:id="@+id/latestAudioFragment"
        android:name="com.example.theirfather.LatestAudioFragment"
        android:label="fragment_latest_audio"
        tools:layout="@layout/fragment_latest_audio" />
    <fragment
        android:id="@+id/browseAudioFragment"
        android:name="com.example.theirfather.BrowseAudioFragment"
        android:label="fragment_browse_audio"
        tools:layout="@layout/fragment_browse_audio" />
</navigation>